@model IEnumerable<MvcExample.Models.Person>
<!DOCTYPE html>
<html>
<head>
    <title>GetSome</title>
</head>
<body>

    <table>
        @foreach (var p in Model)
        {
            <tr>
                <td>@p.FirstName</td>
                <td>@p.LastName</td>
                <td>@p.Address</td>

            </tr>
        }
    </table>
    @Html.ActionLink("Powrót do listy", "Index")

</body>
</html>